{
    "name": "happyr/google-analytics-bundle",
    "type": "symfony-bundle",
    "description": "The Google Analytics Bundle lets you send data (like event tracking) to Google.",
    "keywords": ["Google", "Analytics", "Google Analytics"],
    "homepage": "http://developer.happyr.com/symfony2-bundles/google-analytics-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias@happyr.com"
        },
        {
            "name": "NIWA Software Devloper Team",
            "email": "sdt@niwa.co.nz"
        },
        {
            "name": "Gaspar Zaragoza",
            "email": "gasparmdq@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "http://svn.niwa.co.nz/repos/sdt/symfony_bundles/UtilitiesBundle"
        }
    ],
    "require": {
        "php": ">=5.3.17",
        "symfony/yaml": "2.*",
        "symfony/framework-bundle": "~2.3",
        "niwa/utilities-bundle": "^1.01.0"
    },
    "suggest": {
        "happyr/google-site-authenticator-bundle": "To be able to fetch data from google analytics."
    },
    "autoload": {
        "psr-0": {
            "Happyr\\Google\\AnalyticsBundle\\": ""
        }
    },
    "config": {
        "secure-http": false
    },
    "target-dir": "Happyr/Google/AnalyticsBundle"
}

